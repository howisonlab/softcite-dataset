<article xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:mml="http://www.w3.org/1998/Math/MathML" article-type="research-article"><?properties open_access?><front><journal-meta><journal-id journal-id-type="nlm-ta">Nucleic Acids Res</journal-id><journal-id journal-id-type="publisher-id">nar</journal-id><journal-id journal-id-type="hwp">nar</journal-id><journal-title-group><journal-title>Nucleic Acids Research</journal-title></journal-title-group><issn pub-type="ppub">0305-1048</issn><issn pub-type="epub">1362-4962</issn><publisher><publisher-name>Oxford University Press</publisher-name></publisher></journal-meta><article-meta><article-id pub-id-type="pmid">21051348</article-id><article-id pub-id-type="pmc">3013677</article-id><article-id pub-id-type="doi">10.1093/nar/gkq1073</article-id><article-id pub-id-type="publisher-id">gkq1073</article-id><article-categories><subj-group subj-group-type="heading"><subject>Articles</subject></subj-group></article-categories><title-group><article-title>ASPicDB: a database of annotated transcript and protein variants generated by alternative splicing</article-title></title-group><contrib-group><contrib contrib-type="author"><name><surname>Martelli</surname><given-names>Pier L.</given-names></name><xref ref-type="aff" rid="AFF1"><sup>1</sup></xref></contrib><contrib contrib-type="author"><name><surname>D&#x02019;Antonio</surname><given-names>Mattia</given-names></name><xref ref-type="aff" rid="AFF1"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>Bonizzoni</surname><given-names>Paola</given-names></name><xref ref-type="aff" rid="AFF1"><sup>3</sup></xref></contrib><contrib contrib-type="author"><name><surname>Castrignan&#x000f2;</surname><given-names>Tiziana</given-names></name><xref ref-type="aff" rid="AFF1"><sup>2</sup></xref></contrib><contrib contrib-type="author"><name><surname>D&#x02019;Erchia</surname><given-names>Anna M.</given-names></name><xref ref-type="aff" rid="AFF1"><sup>4</sup></xref></contr