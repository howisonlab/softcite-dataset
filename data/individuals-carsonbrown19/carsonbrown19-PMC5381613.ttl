# https://howisonlab.github.io/softcite-dataset/pdf-files/pmc_oa_files/PMC5381613.pdf
pmcid:PMC5381613 rdf:type bioj:article ;

    citec:has_in_text_mention pmcid:PMC5381613_CB01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "carsonbrown19" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC5381613_CB01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The selection
was based on the following criteria (in order of importance): (1)
similarity of the reactions catalyzed by the candidate enzymes to the
desired reactions, as shown in the Kyoto Encyclopedia of Genes and
Genomes (KEGG) database or in published literature; (2) ability to be
actively expressed in E. coli; and (3) homology of species with E. coli.
Enzymes from gram-negative strains were preferred to those from
gram-positive ones for example.""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "carsonbrown19" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """other"""  ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """KEGG is a database, not actually software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "carsonbrown19" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
