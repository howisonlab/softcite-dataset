pmcid:PMC4621721 rdf:type bioj:article ;

    citec:has_in_text_mention pmcid:PMC4621721_EK01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "evelynkue" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC4621721_EK01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """These search words were entered into PubMed and Google Scholar to locate all relevant studies""" ; # use triple quotes

    citec:on_pdf_page 3  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "evelynkue" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """other""" ; # put one in triple quotes
                           ca:certainty 6 ; # integer
                           ca:memo """According to Google, PubMed is a database and therefore not counted as software.""" ; # use triple quotes
                         ] ;
        ] ;
.
