# https://github.com/howisonlab/softcite-pdf-files/blob/master/docs/pdf-files/economics_pdf_files/10.1080%252F00036846.2016.1218430.pdf
# also https://doi.org/10.1080%2F00036846.2016.1218430
doi:10.1080%2F00036846.2016.1218430 rdf:type bioj:article ;
    rdf:type bioj:econ_article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can code
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent true ; # true if standard article
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1080%2F00036846.2016.1218430_MS01 ;
    # create name for in_text_mention like
    # doi:10.1080%2F00036846.2016.1218430_JH01

    # citations like:
    # doi-cited:10.1080%2F00036846.2016.1218430_AuthorYear
.
doi:10.1080%2F00036846.2016.1218430_MS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The software package AMOS (Analysis of MomentStructures) by Arbuckle (2006) was used to analysethe data.""" ; # use triple quotes

    citec:on_pdf_page 7  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label  """software"""  ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """it says software package""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """AMOS""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Arbuckle""" ; # use triple quotes
                         ] ;
        ] ;
    citec:has_reference doi-cited:10.1080%2F00036846.2016.1218430_Arbuckle-2006 ; # name reference like doi-cited:a2004-40-NAT_GENET_Author-YYYY, no quotes
.
doi-cited:10.1080%2F00036846.2016.1218430_Arbuckle-2006 rdf:type citec:reference ;
    citec:full_quote """Arbuckle,  J.  L.2006.AMOS  (Version  7.0),  ComputerProgram. Chicago, IL: SPSS.""" ; # use triple quotes

    citec:on_pdf_page 12  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:reference_type ;
                           rdfs:label   """user_guide"""  ; # put one in triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """AMOS""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Version  7.0""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent true ; # true/false
                           rdfs:label """2006""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Arbuckle""" ; # use triple quotes
                         ] ;
        ] ;
.
