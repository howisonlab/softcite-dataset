# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC3819448.pdf
pmcid:PMC3819448 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
