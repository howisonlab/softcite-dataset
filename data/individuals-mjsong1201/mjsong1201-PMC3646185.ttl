pmcid:PMC3646185 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC3646185_MS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC3646185_MS02 ;
.
pmcid:PMC3646185_MS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """All analyses were performed in Microsoft Excel 2007and SAS V.9.2 (SAS Institute, Cary, North Carolina,USA) using descriptive statistical methods.""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label  """software"""  ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """it is MS software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
