# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC5018359.pdf
pmcid:PMC5018359 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC5018359_HR01  ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC5018359_HR02  ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.
pmcid:PMC5018359_HR01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """  Allstatistical  analyses  were  conducted  using  statistical  soft-ware (SPSS Inc., version 21, Chicago, IL and Stata software,version 12, StataCorp, Texas, US""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """says statistical software in quote""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent true ; # true/false
                           rdfs:label """version 21""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "henryrossiter" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """SPSS Inc.""" ; # use triple quotes
                         ] ;
        ] ;
.
