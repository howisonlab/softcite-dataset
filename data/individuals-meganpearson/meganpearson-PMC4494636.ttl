# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC4494636.pdf
pmcid:PMC4494636 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC4494636_MP01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC4494636_MP02 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.

pmcid:PMC4494636_MP01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Data gathered were encoded in a Microsoft Excel spreadsheet""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 8 ; # integer
                           ca:memo """pretty sure this qualifies""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Excel""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "meganpearson" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Microsoft""" ; # use triple quotes
                         ] ;
        ] ;
.

pmcid:PMC4494636_MP02 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """ data analysis was performed using SPSS for Windows, version21.0 (SPSS, Chicago, IL, US""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
