
@prefix xsd:   <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .

@prefix ca: <http://floss.syr.edu/ontologies/2008/4/contentAnalysis.owl#> .
@prefix doap: <http://usefulinc.com/ns/doap#> .
@prefix vivo: <http://vivoweb.org/ontology/core#> .

@prefix bioj: <http://james.howison.name/ontologies/bio-journal-sample#> .
@prefix citec: <http://james.howison.name/ontologies/software-citation-coding#> .
@prefix bioj-cited: <http://james.howison.name/ontologies/bio-journal-sample-citation#> .
@prefix pmcid: <https://www.ncbi.nlm.nih.gov/pmc/articles/> .
@prefix pmcid-cited: <http://james.howison.name/ontologies/pmcid-journal-sample-citation#> .
@prefix doi: <http://doi.org/> .
@prefix doi-cited: <http://james.howison.name/ontologies/doi-journal-sample-citation#> .

@prefix dc: <http://dublincore.org/documents/2012/06/14/dcmi-terms/> .

# https://github.com/howisonlab/softcite-pdf-files/blob/master/docs/pdf-files/economics_pdf_files/10.1017%252Fs136510051300076x.pdf
# also https://doi.org/10.1017%2Fs136510051300076x
doi:10.1017%2Fs136510051300076x rdf:type bioj:article ;
    rdf:type bioj:econ_article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can code
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent false ; # true if standard article
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1017%2Fs136510051300076x_JWC01 ;
    # create name for in_text_mention like
    # doi:10.1017%2Fs136510051300076x_JH01

    # citations like:
    # doi-cited:10.1017%2Fs136510051300076x_AuthorYear
.
doi:10.1017%2Fs136510051300076x_JWC01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """you will need to use the Adobe Acrobat Reader software.If you do not have a copy already,""" ; # use triple quotes

    citec:on_pdf_page 4  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """called software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Acrobat Reader""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jackchristen" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Adobe""" ; # use triple quotes
                         ] ;
        ] ;
.
