# https://howisonlab.github.io/softcite-dataset/pdf-files/pmc_oa_files/PMC4450657.pdf
pmcid:PMC4450657 rdf:type bioj:article ;

    citec:has_in_text_mention pmcid:PMC4450657_AN_01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ankimnguyen" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC4450657_AN_01 rdf:type citec:in_text_mention ; # use in text mention name
      citec:full_quote """And 2Ã—2 analysis of variance (ANOVA) was used to examine effect of the mediation intervention""" ; # use triple quotes

      citec:on_pdf_page 3  ; # integer

      citec:spans_pages false ; # true/false

      ca:isTargetOf
          [ rdf:type ca:CodeApplication ;
            ca:hasCoder "ankimnguyen" ;
            ca:appliesCode [ rdf:type citec:mention_type ;
                             rdfs:label """statistical test""" ; # put one in triple quotes
                             ca:certainty 10 ; # integer
                             ca:memo """Google search clarified that it was a statistical test.""" ; # use triple quotes
                           ] ;
          ] ;
      ca:isTargetOf
          [ rdf:type ca:CodeApplication ;
            ca:hasCoder "ankimnguyen" ;
            ca:appliesCode [ rdf:type citec:software_was_used ;
                             citec:isPresent false; # true/false
                           ] ;
          ] ;

.
