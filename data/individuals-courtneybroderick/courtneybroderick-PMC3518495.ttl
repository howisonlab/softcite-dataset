# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC3518495.pdf
pmcid:PMC3518495 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC3518495_CB01 ;
    citec:has_in_text_mention pmcid:PMC3518495_CB02 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.
pmcid:PMC3518495_CB01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Gene expression was compared with the web-based software
package for the PCR Array System (http://www.superarray.com/
pcr/arrayanalysis.php), which automatically performs all ΔΔCt
based fold-change calculations from the specific uploaded raw
threshold cycle data.""" ; # use triple quotes

    citec:on_pdf_page 11  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 8 ; # integer
                           ca:memo """This is obviously a software mention, but I am unclear if the software's name is PCR Array System, or if they are just referencing a software they don't name.  Google search did not help.""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent true ; # true/false
                           rdfs:label """(http://www.superarray.com/
pcr/arrayanalysis.php),""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "courtneybroderick" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
.
