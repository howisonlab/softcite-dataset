    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC5329793_MZ01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """All the data was analyzed by stata11.0 soft-ware (http://www.stata.com; Stata Corporation, College Station, TX).""" ; # use triple quotes

