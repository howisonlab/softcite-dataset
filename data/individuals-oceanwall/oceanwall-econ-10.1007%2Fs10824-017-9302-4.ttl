# https://github.com/howisonlab/softcite-pdf-files/blob/master/docs/pdf-files/economics_pdf_files/10.1007%252Fs10824-017-9302-4.pdf
# also https://doi.org/10.1007%2Fs10824-017-9302-4
doi:10.1007%2Fs10824-017-9302-4 rdf:type bioj:article ;
    rdf:type bioj:econ_article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can code
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent true ; # true if standard article
                         ] ;
        ] ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1007%2Fs10824-017-9302-4_MZ01 ;
    # create name for in_text_mention like
    # doi:10.1007%2Fs10824-017-9302-4_JH01

    # citations like:
    # doi-cited:10.1007%2Fs10824-017-9302-4_AuthorYear
.
doi:10.1007%2Fs10824-017-9302-4_MZ01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The data were collectedby computer aided personal interviewing (for details of the sample design see TNS-BMRB2012).""" ; # use triple quotes

    citec:on_pdf_page 6  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 7 ; # integer
                           ca:memo """refers to use of computers to aid interviewing process; implicitly refers to software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """TNS-BMRB""" ; # use triple quotes
                         ] ;
        ] ;
    citec:has_reference doi-cited:10.1007%2Fs10824-017-9302-4_TNSBMRB2012
.
doi-cited:10.1007%2Fs10824-017-9302-4_TNSBMRB2012 rdf:type citec:reference ;
    citec:full_quote """TNS BMRB (2012) 2012/13Taking part survey technical report.Accessed December 12, 2016,http://doc.ukdataservice.ac.uk/doc/7371/mrdoc/pdf/7371_taking_part_y8_technical_report.pdf.""" ; # use triple quotes

    citec:on_pdf_page 16  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:reference_type ;
                           rdfs:label """publication""" ; # put one in triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:version_date ;
