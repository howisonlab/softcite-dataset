pmcid:PMC4160583 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mrcyndns" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC4160583_MD01 ;
    citec:has_in_text_mention pmcid:PMC4160583_MD02 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.
pmcid:PMC4160583_MD01  rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Plasma glucose levels, serum total cholesterol (TC), TG, HDL-C, LDL-cholesterol (LDL-C), and serum uric acid (UA) were determined using standard methods (Hitachi 47 automatic analyzer; Hitachi, Tokyo, Japan).""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mrcyndns" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label   """hardware """ ; # put one in triple quotes
                           ca:certainty 8 ; # integer
                           ca:memo """I looked up the Hitachi 47 automatic analyzer and I did not find the specific one but I did find the hitachi website that had a lot of machines that did clinical analyses""" ; # use triple quotes
                         ] ;
        ] ;
.
pmcid:PMC4160583_MD02 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Statistical analyses were performed using the SPSS version 21.0 (IBM Co., Armonk, NY, USA). """ ; # use triple quotes

