# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/10.1257%2Faer.20140850.pdf
doi:10.1257%2Faer.20140850 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:codable ;
                           citec:isPresent true; # true if can't code
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:standard_type ;
                           citec:isPresent true ; # true if standard article
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention doi:10.1257%2Faer.20140850_CT01 ;
    citec:has_in_text_mention doi:10.1257%2Faer.20140850_CT02 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
.
doi:10.1257%2Faer.20140850_CT01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """This h-index adjustment is suggested by Kaur, Radicchi, and Menczer(2013) and it is used by the online citation analysis tool Scholarometer(http://scholarometer.indiana.edu), whose popularity fo comoputing and comparing the h-indices of scholars acroos disciplines seems to be growing.""" ; # use triple quotes

    citec:on_pdf_page 10  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 8 ; # integer
                           ca:memo """Mentions that it is a online citation tool""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Scholarometer""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent true ; # true/false
                           rdfs:label """http://scholarometer.indiana.edu""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
.
doi:10.1257%2Faer.20140850_CT02 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """ Citation data for each economist
were collected from Google Scholar""" ; # use triple quotes

    citec:on_pdf_page 11  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 7 ; # integer
                           ca:memo """Google mentions that it is a
                           web search engine that indexes full text or metadata for scholarly literature """ ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true; # true/false
                           rdfs:label """Google Scholar""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ctjoe" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
.
