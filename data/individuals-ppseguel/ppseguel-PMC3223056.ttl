# https://howisonlab.github.io/softcite-dataset/pdf-files/pmc_oa_files/PMC3223056.pdf
pmcid:PMC3223056 rdf:type bioj:article ;

    citec:has_in_text_mention pmcid:PMC3223056_PS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ppseguel" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC3223056_PS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The genomic, cDNA and protein sequences ofA. thaliana FRI(At4g00650) were used as queries for BLASTN and TBLASTNhomology searches of the nt/nr and EST sections of GenBank (asof 27 November 2005), which identified aB. napusEST (GenBankaccession number CD836567, derived fromB. napuscv. Jet Neuf)with 74% sequence identity over 578 bp ofFRIexon 1. A genomicfragment corresponding to the EST was amplified fromB. napuscv. Express genomic DNA using an EST sequence-derived primerpair (A102â€“A103; seeSupplementary Table S2atJXBonline).""" ; # use triple quotes

    citec:on_pdf_page 4  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "ppseguel" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """other""" ; # put one in triple quotes
                           ca:certainty 7 ; # integer
                           ca:memo """I use google to search it, and it seems is a databse repository and it work as a platform to share information about genes. https://www.ncbi.nlm.nih.gov/genbank/""" ; # use triple quotes
                         ] ;
        ] ;
