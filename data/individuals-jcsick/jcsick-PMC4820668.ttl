# https://howisonlab.github.io/softcite-dataset/pdf-files/pmc_oa_files/PMC4820668.pdf
pmcid:PMC4820668 rdf:type bioj:article ;

    citec:has_in_text_mention pmcid:PMC4820668_JS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;
.
pmcid:PMC4820668_JS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """We calculated 95% cis for the POr using the maximum likelihood odds ratio with mid-P using Statcalc in epi info 7 and for irr based on the Poisson distribution indicated in the geigy scientific tables.23""" ; # use triple quotes

    citec:on_pdf_page 4  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 9 ; # integer
                           ca:memo """google search yielded the info that Statcalc is statistics software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true; # true/false
                           rdfs:label """Statcalc""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "jcsick" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent false ; # true/false
                            # use triple quotes
                         ] ;
        ] ;
     # name reference like pmcid-cited:a2004-40-NAT_GENET_Author-YYYY, no quotes
.
