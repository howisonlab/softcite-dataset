                           citec:isPresent true ; # true/false
                           rdfs:label """ImageJ""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sehreenali" ;
