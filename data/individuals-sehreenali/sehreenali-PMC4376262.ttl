.
pmcid:PMC4376262_SA01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """The relative intensities of the target genes (the genes for albumin, hepatocyte nuclear factor-4 alpha [HNF-4α], and ASGPR) were calculated with ImageJ image analysis software (Version 1.48) and were compared to a housekeeping marker,glyceraldehyde 3-phosphate dehydrogenase. """; # use triple quotes

    citec:on_pdf_page  3 ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sehreenali" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """Article mentioned it was a software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "sehreenali" ;
